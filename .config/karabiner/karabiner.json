{
    "global": { "show_in_menu_bar": false },
    "profiles": [
        {
            "complex_modifications": {
                "rules": [
                    {
                        "description": "Right Option as Hyper, Menu key in Windows",
                        "manipulators": [
                            {
                                "conditions": [
                                    {
                                        "bundle_identifiers": [
                                            "^com\\.microsoft\\.rdc\\.macos$",
                                            "^com\\.moonlight\\-stream\\.Moonlight$",
                                            "^com\\.apple\\.Safari\\.WebApp\\.4A4AAC77\\-620F\\-4C1B\\-B4A0\\-5DAAED400810$"
                                        ],
                                        "type": "frontmost_application_unless"
                                    }
                                ],
                                "from": { "key_code": "right_option" },
                                "to": [
                                    {
                                        "key_code": "left_command",
                                        "modifiers": ["left_control", "left_shift", "left_option"]
                                    }
                                ],
                                "type": "basic"
                            },
                            {
                                "conditions": [
                                    {
                                        "bundle_identifiers": [
                                            "^com\\.microsoft\\.rdc\\.macos$",
                                            "^com\\.moonlight\\-stream\\.Moonlight$",
                                            "^com\\.apple\\.Safari\\.WebApp\\.4A4AAC77\\-620F\\-4C1B\\-B4A0\\-5DAAED400810$"
                                        ],
                                        "type": "frontmost_application_if"
                                    }
                                ],
                                "from": { "key_code": "right_option" },
                                "to": [{ "key_code": "application" }],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Right Command changes input language ",
                        "manipulators": [
                            {
                                "conditions": [
                                    {
                                        "bundle_identifiers": [
                                            "^com\\.microsoft\\.rdc\\.macos$",
                                            "^com\\.moonlight\\-stream\\.Moonlight$",
                                            "^com\\.apple\\.Safari\\.WebApp\\.4A4AAC77\\-620F\\-4C1B\\-B4A0\\-5DAAED400810$"
                                        ],
                                        "type": "frontmost_application_unless"
                                    }
                                ],
                                "from": { "key_code": "right_command" },
                                "to": [{ "key_code": "f18" }],
                                "type": "basic"
                            },
                            {
                                "conditions": [
                                    {
                                        "bundle_identifiers": [
                                            "^com\\.microsoft\\.rdc\\.macos$",
                                            "^com\\.moonlight\\-stream\\.Moonlight$",
                                            "^com\\.apple\\.Safari\\.WebApp\\.4A4AAC77\\-620F\\-4C1B\\-B4A0\\-5DAAED400810$"
                                        ],
                                        "type": "frontmost_application_if"
                                    }
                                ],
                                "from": { "key_code": "right_command" },
                                "to": [{ "key_code": "right_option" }],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Toggle Mic Mute with F17",
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "f17",
                                    "modifiers": { "optional": ["any"] }
                                },
                                "to": [{ "shell_command": "osascript ~/scripts/toggle_mic_mute.scpt" }],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Remap PrintScr from LofreeLite to F16",
                        "manipulators": [
                            {
                                "conditions": [
                                    {
                                        "identifiers": [
                                            {
                                                "product_id": 591,
                                                "vendor_id": 1452
                                            }
                                        ],
                                        "type": "device_if"
                                    }
                                ],
                                "from": {
                                    "modifiers": { "optional": ["any"] },
                                    "simultaneous": [
                                        { "key_code": "left_command" },
                                        { "key_code": "left_shift" },
                                        { "key_code": "3" }
                                    ]
                                },
                                "to": [{ "key_code": "f16" }],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Hyper + L triggers Cmd + Shift + L in Safari (Bitwarden Autofill)",
                        "manipulators": [
                            {
                                "conditions": [
                                    {
                                        "bundle_identifiers": [
                                            "^com\\.apple\\.Safari$"
                                        ],
                                        "type": "frontmost_application_if"
                                    }
                                ],
                                "from": {
                                    "key_code": "l",
                                    "modifiers": { "mandatory": ["left_command", "left_control", "left_option", "left_shift"] }
                                },
                                "to": [
                                    {
                                        "key_code": "l",
                                        "modifiers": ["left_command", "left_shift"]
                                    }
                                ],
                                "type": "basic"
                            }
                        ]
                    }
                ]
            },
            "devices": [
                {
                    "identifiers": { "is_keyboard": true },
                    "simple_modifications": [
                        {
                            "from": { "key_code": "left_control" },
                            "to": [{ "apple_vendor_top_case_key_code": "keyboard_fn" }]
                        },
                        {
                            "from": { "apple_vendor_top_case_key_code": "keyboard_fn" },
                            "to": [{ "key_code": "left_control" }]
                        }
                    ]
                },
                {
                    "disable_built_in_keyboard_if_exists": true,
                    "identifiers": {
                        "is_keyboard": true,
                        "product_id": 591,
                        "vendor_id": 1452
                    },
                    "simple_modifications": [
                        {
                            "from": { "key_code": "insert" },
                            "to": [{ "key_code": "f17" }]
                        }
                    ]
                },
                {
                    "identifiers": {
                        "is_keyboard": true,
                        "product_id": 666,
                        "vendor_id": 1452
                    },
                    "ignore": true
                }
            ],
            "fn_function_keys": [
                {
                    "from": { "key_code": "f5" },
                    "to": [{ "key_code": "f17" }]
                }
            ],
            "name": "Default profile",
            "selected": true,
            "virtual_hid_keyboard": { "keyboard_type_v2": "ansi" }
        }
    ]
}